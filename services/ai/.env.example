# ==============================================
# ZipCheck AI Service - Environment Variables
# ==============================================

# ----------------------------------------------
# Database Configuration
# ----------------------------------------------
# Supabase PostgreSQL connection URL with pgvector extension
DATABASE_URL=postgresql://user:password@host:5432/database

# ----------------------------------------------
# LLM Provider API Keys
# ----------------------------------------------
# OpenAI API key for GPT models and embeddings
OPENAI_API_KEY=sk-...

# Anthropic API key for Claude models
ANTHROPIC_API_KEY=sk-ant-...

# ----------------------------------------------
# LLM Configuration
# ----------------------------------------------
# Primary LLM provider: "openai" or "claude"
PRIMARY_LLM=openai

# Judge LLM for consensus mode: "openai" or "claude"
JUDGE_LLM=claude

# ----------------------------------------------
# OpenAI Model Strategy (Standard Tier)
# ----------------------------------------------
# GPT-4o for vision/OCR + Structured Outputs (scanned PDFs)
OPENAI_VISION_MODEL=gpt-4o

# GPT-4o-mini for fast classification/validation
OPENAI_CLASSIFICATION_MODEL=gpt-4o-mini

# Default analysis model (can upgrade to gpt-4o for complex cases)
OPENAI_ANALYSIS_MODEL=gpt-4o-mini

# ----------------------------------------------
# Embedding Configuration
# ----------------------------------------------
# Embedding model: text-embedding-3-small (1536D) or -large (3072D)
EMBED_MODEL=text-embedding-3-small

# Embedding dimensions (1536 for small, 3072 for large)
EMBED_DIMENSIONS=1536

# ----------------------------------------------
# Model Parameters
# ----------------------------------------------
# LLM generation temperature (0.0 - 2.0)
LLM_TEMPERATURE=0.2

# Maximum tokens for LLM response
LLM_MAX_TOKENS=2048

# ----------------------------------------------
# Vision API Configuration
# ----------------------------------------------
# Maximum tokens for vision API responses
VISION_MAX_TOKENS=4096

# Vision API detail level: "low", "high", or "auto"
# (high for OCR, low for cost savings)
VISION_DETAIL=high

# ----------------------------------------------
# API Configuration
# ----------------------------------------------
# CORS allowed origins (comma-separated or "*" for all)
AI_ALLOWED_ORIGINS=http://localhost:3000,https://zipcheck.app

# ----------------------------------------------
# Redis (Optional - for task queue)
# ----------------------------------------------
# Redis URL for Celery/RQ task queue
REDIS_URL=redis://localhost:6379/0

# ----------------------------------------------
# Observability & Monitoring
# ----------------------------------------------
# Sentry DSN for error tracking (optional)
SENTRY_DSN=https://...@sentry.io/...

# Langfuse keys for LLM observability (optional)
LANGFUSE_PUBLIC_KEY=pk-lf-...
LANGFUSE_SECRET_KEY=sk-lf-...
LANGFUSE_HOST=https://cloud.langfuse.com

# ----------------------------------------------
# Application Settings
# ----------------------------------------------
# Application environment: development, staging, production
APP_ENV=development

# Logging level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Google Gemini API key
GEMINI_API_KEY=...

# ----------------------------------------------
# Public Data API Keys
# ----------------------------------------------
# 도로명주소 검색 API 키 (행정안전부)
KEYWORD_JUSO_API_KEY=your_juso_api_key_here

# 공공데이터포털 API 키 (국토부 - 실거래가, 법정동코드)
DATA_GO_KR_API_KEY=your_data_go_kr_api_key_here

# 건축물대장 API 키 (국토부)
BUILDING_LEDGER_API_KEY=5c92614d0bd229ae3a8c329ac4c354f7158b91c051b9d0d4455f766ee9f0d7b4

# ----------------------------------------------
# OAuth Configuration
# ----------------------------------------------
# Kakao OAuth
KAKAO_CLIENT_ID=your_kakao_client_id
KAKAO_REDIRECT_URI=http://localhost:8000/api/auth/kakao/callback

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# Naver OAuth
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret
NAVER_REDIRECT_URI=http://localhost:3000/auth/callback

# ----------------------------------------------
# Supabase Configuration
# ----------------------------------------------
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

